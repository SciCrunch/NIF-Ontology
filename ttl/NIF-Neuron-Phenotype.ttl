    rdfs:subClassOf ilxtr:SpikingPhenotype ;
    owl:disjointWith ilxtr:NonAccomodatingSpikingPhenotype ;
    ilxtr:useObjectProperty ilxtr:hasElectrophysiologicalPhenotype ;
    skos:hiddenLabel "Accomodating Spiking" .

