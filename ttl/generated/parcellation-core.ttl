<https://raw.githubusercontent.com/SciCrunch/NIF-Ontology/atlas/ttl/generated/parcellation-core.ttl> a owl:Ontology ;
    owl:imports <http://ontology.neuinfo.org/NIF/ttl/generated/parcellation-artifacts.ttl> ;
    rdfs:label "Parcellation Core" ;
    rdfs:comment "Core OWL2 entities needed for parcellations" ;
    owl:versionInfo "2021-02-11" ;
    prov:wasGeneratedBy <https://github.com/tgbugs/pyontutils/blob/305a5d3c5cc18870a9674647db7461bb8b2e70e4/nifstd/nifstd_tools/parcellation/__init__.py#L620> .

### Object Properties

ilxtr:labelPartOf a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:subPropertyOf BFO:0000050 .

parc:annotates a owl:ObjectProperty ;
    rdfs:label "annotates" ;
    NIFRID:synonym "isAnchoredTo",
        "isCoordinatedTo" ;
    rdfs:domain parc:Annotation ;
    rdfs:range parc:Space .

parc:hasResolution a owl:ObjectProperty ;
    rdfs:label "hasResolution" ;
    rdfs:domain parc:Data ;
    rdfs:range parc:Resolution .

parc:hasSpace a owl:ObjectProperty ;
    rdfs:label "hasSpace" ;
    rdfs:domain parc:Data ;
    rdfs:range parc:Space .

parc:hasUnit a owl:ObjectProperty ;
    rdfs:label "hasUnit" .

parc:isMeasurementOf a owl:ObjectProperty ;
    rdfs:domain parc:Data .

parc:names a owl:ObjectProperty ;
    rdfs:label "names" ;
    rdfs:domain parc:Region ;
    rdfs:range parc:Annotation .

parc:transforms a owl:ObjectProperty ;
    rdfs:domain parc:TransformationRule ;
    rdfs:range parc:Space .

### Data Properties

parc:hasResolutionX a owl:DatatypeProperty .

parc:hasResolutionY a owl:DatatypeProperty .

parc:hasResolutionZ a owl:DatatypeProperty .

### Classes

ilxtr:parcellationLabel a owl:Class ;
    owl:equivalentClass parc:Label .

ilxtr:parcellationRegion a owl:Class ;
    owl:equivalentClass parc:Region .

parc:Alignment a owl:Class ;
    rdfs:label "spatial alignment" ;
    definition: "dataset -> dataset, rotation, translation" ;
    rdfs:subClassOf parc:Process,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:SpacePreservingLinearTransformation ],
        [ a owl:Restriction ;
            owl:onProperty ilxtr:hasInformationOutput ;
            owl:someValuesFrom parc:TransformationRule ] ;
    ilxtr:localName "alignment" .

parc:AnatomicalAxis a owl:Class ;
    rdfs:subClassOf parc:Axis .

parc:AnatomicalAxisAlignmentVector a owl:Class ;
    rdfs:label "anatomical axis alignment vector" ;
    rdfs:subClassOf parc:Vector,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirectionMappedToPositiveX ;
            owl:someValuesFrom parc:AnatomicalDirection ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirectionMappedToPositiveY ;
            owl:someValuesFrom parc:AnatomicalDirection ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirectionMappedToPositiveZ ;
            owl:someValuesFrom parc:AnatomicalDirection ] ;
    ilxtr:localName "anatomical axis alignment vector" .

parc:AnatomicalDataset a owl:Class ;
    rdfs:label "anatomical dataset" ;
    NIFRID:synonym "default image",
        "raw image",
        "template",
        "uncoordinated dataset",
        "unoriented dataset" ;
    rdfs:subClassOf parc:Data,
        [ a owl:Restriction ;
            owl:onProperty parc:hasSpace ;
            owl:someValuesFrom parc:ImageSpace ] ;
    ilxtr:localName "anatomical dataset" .

parc:AnatomicalDirection a owl:Class ;
    rdfs:subClassOf parc:Direction .

parc:Anchoring a owl:Class ;
    rdfs:label "anchoring" ;
    NIFRID:synonym "RegistrationUsingAnnotationOnly" ;
    definition: "dataset -> annotation" ;
    rdfs:subClassOf parc:Process ;
    ilxtr:localName "anchoring" .

parc:Annotation a owl:Class ;
    rdfs:label "parcellation annotation" ;
    NIFRID:synonym "atlas annotation" ;
    rdfs:subClassOf parc:Artifact,
        [ a owl:Restriction ;
            owl:onProperty parc:annotates ;
            owl:someValuesFrom parc:Space ;
            owl:cardinality 1 ] ;
    ilxtr:localName "annotation" .

parc:AnnotationCriteria a owl:Class ;
    rdfs:label "anatomical annotation criteria" ;
    NIFRID:synonym "anatomical delineation criteria",
        "delineation criteria",
        "parcellation annotation criteria" ;
    rdfs:subClassOf parc:Artifact ;
    ilxtr:localName "annotation criteria" .

parc:AnnotationCriteriaApplication a owl:Class ;
    rdfs:label "annotation criteria application" ;
    rdfs:subClassOf parc:Process ;
    ilxtr:localName "annotation criteria application" .

parc:AnnotationSet a owl:Class ;
    rdfs:label "anatomical annotation set" ;
    NIFRID:synonym "annotation set",
        "parcellation annotation set" ;
    definition: "A set of anatomical annotations that are anchored in the same space." ;
    rdfs:subClassOf parc:Artifact,
        [ a owl:Restriction ;
            owl:onProperty hasMember: ;
            owl:someValuesFrom parc:Annotation ] ;
    editorNote: "It is probably the case that the hasMember Annotation restriction could be an equivalent class axiom." ;
    ilxtr:localName "annotation set" .

parc:Anterior a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Posterior ] .

parc:AnteriorPosteriorAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Anterior ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Posterior ] .

parc:Artifact a owl:Class ;
    owl:equivalentClass ilxtr:parcellationArtifact ;
    rdfs:label "parcellation artifact" ;
    NIFRID:synonym "parcellation artifact" ;
    ilxtr:localName "artifact" .

parc:Atlas a owl:Class ;
    owl:equivalentClass ilxtr:parcellationAtlas ;
    rdfs:label "parcellation atlas" ;
    NIFRID:synonym "anatomical reference atlas" ;
    rdfs:subClassOf parc:Artifact,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:AnnotationSet ],
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:Space ],
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:Terminology ] ;
    ilxtr:localName "atlas" .

parc:Axis a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Direction ;
            owl:cardinality 2 ] .

parc:AxisDirectionMatrix a owl:Class ;
    rdfs:subClassOf parc:Matrix .

parc:AxisOrigin a owl:Class ;
    rdfs:subClassOf parc:Vector ;
    editorNote: "" .

parc:Border a owl:Class ;
    rdfs:label "anatomical border" ;
    rdfs:subClassOf parc:Line ;
    ilxtr:localName "border" .

parc:Boundary a owl:Class ;
    rdfs:label "anatomical boundary" ;
    rdfs:subClassOf parc:ClosedCurve ;
    ilxtr:localName "boundary" .

parc:Caudal a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Rostral ] .

parc:Centroid a owl:Class ;
    rdfs:label "anatomical centroid" ;
    rdfs:subClassOf parc:Point ;
    ilxtr:localName "centroid" .

parc:ClosedCurve a owl:Class ;
    rdfs:label "anatomical closed curve" ;
    rdfs:subClassOf parc:Annotation ;
    ilxtr:localName "closed curve" .

parc:CoordinatedDataset a owl:Class ;
    rdfs:label "coordinated dataset" ;
    NIFRID:synonym "reconstructed brain" ;
    rdfs:subClassOf parc:Data,
        [ a owl:Restriction ;
            owl:onProperty parc:hasSpace ;
            owl:someValuesFrom parc:Space ] ;
    ilxtr:localName "coordinated dataset" .

parc:CoordinateSystemApplication a owl:Class ;
    rdfs:label "coordinate system application" ;
    rdfs:subClassOf parc:Process,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:Alignment ],
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:Transformation ] ;
    ilxtr:localName "coordinate system application" .

parc:CoordinateSystemDefinition a owl:Class ;
    rdfs:label "coordinate system definition" ;
    NIFRID:synonym "coordination rule" ;
    rdfs:subClassOf parc:Artifact ;
    ilxtr:localName "coordinate system definition" .

parc:Data a owl:Class ;
    rdfs:label "anatomical data" ;
    rdfs:subClassOf parc:Artifact,
        [ a owl:Restriction ;
            owl:onProperty parc:hasResolution ;
            owl:someValuesFrom parc:Resolution ;
            owl:cardinality 1 ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasSpace ;
            owl:someValuesFrom parc:Space ],
        [ a owl:Restriction ;
            owl:onProperty parc:isMeasurementOf ;
            owl:someValuesFrom NCBITaxon:1 ] ;
    ilxtr:localName "data" .

parc:Delineation a owl:Class ;
    rdfs:label "anatomical delineation" ;
    definition: "an annotation with criteria demarcating the border between two regions or the boundary of a single region" ;
    rdfs:subClassOf parc:Annotation ;
    ilxtr:localName "delineation" .

parc:Direction a owl:Class .

parc:Dorsal a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Ventral ] .

parc:DorsalVentralAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Dorsal ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Ventral ] .

parc:ImageSpace a owl:Class ;
    rdfs:label "anatomical image space" ;
    NIFRID:synonym "data space",
        "dataset space",
        "donor space",
        "image space",
        "individual space",
        "intrinsic data space",
        "intrinsic space",
        "sample space",
        "specimen space" ;
    rdfs:subClassOf parc:Space ;
    ilxtr:editorQuestion "are specimen/individual space different from image/dataset space?" ;
    ilxtr:localName "image space" .

parc:Inferior a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Superior ] .

parc:InferiorSuperiorAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Inferior ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Superior ] .

parc:Label a owl:Class ;
    rdfs:label "parcellation label" ;
    NIFRID:synonym "Parcellation Label",
        "parcellation term",
        "term" ;
    skos:definition "Parcellation labels are strings characthers sometimes associated with a unique identifier, such as an index number or an iri." ;
    rdfs:subClassOf UBERON:0001062 ;
    ilxtr:localName "label" .

parc:Landmark a owl:Class ;
    rdfs:label "anatomical landmark" ;
    rdfs:subClassOf parc:Annotation ;
    ilxtr:localName "landmark" .

parc:Lateral a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Medial ] .

parc:Left a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Right ] .

parc:LeftRightAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Left ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Right ] .

parc:Line a owl:Class ;
    rdfs:label "anatomical line" ;
    rdfs:subClassOf parc:Annotation ;
    ilxtr:localName "line" .

parc:LinearRegistration a owl:Class ;
    rdfs:subClassOf parc:Registration,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:LinearTransformation ] .

parc:LinearTransformation a owl:Class ;
    rdfs:subClassOf parc:Transformation .

parc:Matrix a owl:Class ;
    rdfs:label "matrix" ;
    ilxtr:localName "matrix" .

parc:Medial a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Lateral ] .

parc:MedialLateralAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Lateral ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Medial ] .

parc:NegativeX a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:PositiveX ] .

parc:NegativeY a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:PositiveY ] .

parc:NegativeZ a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:PositiveZ ] .

parc:NonLinearRegistration a owl:Class ;
    rdfs:subClassOf parc:Registration,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:NonLinearTransformation ] .

parc:NonLinearTransformation a owl:Class ;
    rdfs:subClassOf parc:Transformation .

parc:Orienting a owl:Class ;
    rdfs:label "spatial orienting" ;
    NIFRID:synonym "orienting" ;
    definition: "axis -> axis" ;
    rdfs:subClassOf parc:Alignment ;
    ilxtr:localName "orienting" .

parc:Parcellation a owl:Class ;
    rdfs:label "anatomical parcellation" ;
    NIFRID:synonym "anatomical parcellation scheme",
        "parcellation scheme" ;
    rdfs:subClassOf parc:AnnotationSet ;
    ilxtr:localName "parcellation" .

parc:Point a owl:Class ;
    rdfs:label "anatomical point" ;
    rdfs:subClassOf parc:Annotation ;
    ilxtr:localName "point" .

parc:PositiveX a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:NegativeX ] .

parc:PositiveY a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:NegativeY ] .

parc:PositiveZ a owl:Class ;
    rdfs:subClassOf parc:SpatialDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:NegativeZ ] .

parc:Posterior a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Anterior ] .

parc:Process a owl:Class ;
    rdfs:label "process" ;
    ilxtr:localName "process" .

parc:Region a owl:Class ;
    rdfs:label "parcellation region" ;
    NIFRID:synonym "atlas region",
        "Parcellation Region" ;
    skos:definition "Parcellation regions are 'anatomical entities' that correspond to some part of a real biological system and are equivalent to an intersection between a parcellation label and a specific version of an atlas that defines or uses that label and that provides a definitive (0, 1, or probabilistic) way to determine whether a particular sample corresponds to any given region. " ;
    rdfs:subClassOf parc:Label,
        [ a owl:Restriction ;
            owl:onProperty parc:names ;
            owl:someValuesFrom parc:Annotation ;
            owl:cardinality 1 ] ;
    ilxtr:localName "region" .

parc:Registration a owl:Class ;
    rdfs:label "spatial registration" ;
    definition: "dataset -> dataset, rotation, translation, scaling, or some nonlinear transformation" ;
    rdfs:subClassOf parc:Alignment,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:Transformation ] ;
    ilxtr:localName "registration" .

parc:Resolution a owl:Class ;
    rdfs:label "resolution" ;
    rdfs:subClassOf parc:Vector,
        [ a owl:Restriction ;
            owl:onProperty parc:hasResolutionX ;
            owl:someValuesFrom owl:real ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasResolutionY ;
            owl:someValuesFrom owl:real ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasResolutionZ ;
            owl:someValuesFrom owl:real ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasUnit ;
            owl:someValuesFrom parc:spaceUnit ] ;
    ilxtr:localName "resolution" .

parc:Right a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Left ] .

parc:Rostral a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Caudal ] .

parc:RostralCaudalAxis a owl:Class ;
    rdfs:subClassOf parc:AnatomicalAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Caudal ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:Rostral ] .

parc:Rotation a owl:Class ;
    rdfs:subClassOf parc:SpacePreservingLinearTransformation .

parc:RotationMatrix a owl:Class ;
    rdfs:subClassOf parc:TransformationMatrix .

parc:Scaling a owl:Class ;
    rdfs:subClassOf parc:LinearTransformation .

parc:Segmentation a owl:Class ;
    rdfs:label "anatomical segmentation" ;
    definition: "an annotation set that may or may not have an explicit bounding region but that does break or separate the area or volume into distinct segments" ;
    rdfs:subClassOf parc:AnnotationSet ;
    editorNote: "This isn't really distinct from an annotation set." ;
    ilxtr:localName "segmentation" .

parc:Space a owl:Class ;
    rdfs:label "anatomical space" ;
    NIFRID:synonym "anatomical coordinate space",
        "atlas space",
        "common coordinate space",
        "parameterized coordinate space",
        "physical space" ;
    rdfs:subClassOf parc:Artifact,
        parc:Space,
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:AnatomicalAxisAlignmentVector ],
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:AxisDirectionMatrix ],
        [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom parc:AxisOrigin ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasUnit ;
            owl:someValuesFrom parc:spaceUnit ] ;
    editorNote: "To avoid problems like those encountered by nifti an anatomical reference space MUST define an anatomical axis alignment vector which states, for example, [X, Y, Z] -> [R, A, D] (left hand rule). This immediately indicates the three annotations required on any unoriented dataset as opposed to someone trying to apply the anatomical directions directly to the unoriented dataset" ;
    ilxtr:localName "space" .

parc:SpacePreservingLinearTransformation a owl:Class ;
    rdfs:subClassOf parc:LinearTransformation .

parc:SpatialAxis a owl:Class ;
    rdfs:subClassOf parc:Axis .

parc:SpatialDirection a owl:Class ;
    rdfs:subClassOf parc:Direction .

parc:Superior a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Inferior ] .

parc:Terminology a owl:Class ;
    owl:equivalentClass ilxtr:parcellationTerminology ;
    rdfs:label "parcellation terminology" ;
    NIFRID:synonym "controlled vocabulary" ;
    rdfs:subClassOf parc:Artifact ;
    ilxtr:localName "terminology" .

parc:Tiling a owl:Class ;
    rdfs:label "anatomical tiling parcellation" ;
    NIFRID:synonym "tiling annotation set",
        "tiling parcellation" ;
    definition: "an annotation set that forms a non-overlapping cover of a single bounding annotation" ;
    rdfs:subClassOf parc:AnnotationSet ;
    editorNote: "closure axioms related to the bounding annotation need to be considered" ;
    ilxtr:localName "tiling" .

parc:Transformation a owl:Class ;
    rdfs:label "spatial transformation" ;
    definition: "space -> space" ;
    rdfs:subClassOf parc:Process,
        [ a owl:Restriction ;
            owl:onProperty ilxtr:hasInformationInput ;
            owl:someValuesFrom parc:Space ],
        [ a owl:Restriction ;
            owl:onProperty ilxtr:hasInformationOutput ;
            owl:someValuesFrom parc:Space ] ;
    ilxtr:localName "transformation" .

parc:TransformationMatrix a owl:Class ;
    rdfs:label "transformation matrix" ;
    rdfs:subClassOf parc:Matrix,
        parc:TransformationRule ;
    ilxtr:localName "transformation matrix" .

parc:TransformationRule a owl:Class ;
    rdfs:label "transformation rule" ;
    rdfs:subClassOf parc:Artifact,
        [ a owl:Restriction ;
            owl:onProperty parc:transforms ;
            owl:someValuesFrom parc:Space ] ;
    ilxtr:localName "transformation rule" .

parc:Translation a owl:Class ;
    rdfs:subClassOf parc:SpacePreservingLinearTransformation .

parc:Vector a owl:Class ;
    rdfs:label "vector" ;
    ilxtr:localName "vector" .

parc:Ventral a owl:Class ;
    rdfs:subClassOf parc:AnatomicalDirection,
        [ a owl:Restriction ;
            owl:onProperty parc:hasOppositeDirection ;
            owl:someValuesFrom parc:Dorsal ] .

parc:XAxis a owl:Class ;
    rdfs:subClassOf parc:SpatialAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:NegativeX ],
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:PositiveX ] .

parc:YAxis a owl:Class ;
    rdfs:subClassOf parc:SpatialAxis,
        [ a owl:Restriction ;
            owl:onProperty parc:hasDirection ;
            owl:someValuesFrom parc:NegativeY ],
        [ a owl:Restriction ;
